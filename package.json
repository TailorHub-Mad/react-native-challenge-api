{
  "name": "tailor_hub_technical_review",
  "version": "1.0.0",
  "description": "template back",
  "main": "src/app.ts",
  "scripts": {
    "test:coverage": "ENV_FILE=.env.test jest --config config/jest.json --collectCoverage",
    "start": "NODE_ENV=production node -r module-alias/register dist/app.js",
    "dev": "NODE_ENV=development nodemon",
    "dev:prod": "NODE_ENV=production nodemon",
    "build": "rm -rf dist && npx tsc --build tsconfig.prod.json",
    "serve": "yarn run build && NODE_ENV=production node -r module-alias/register dist/app.js",
    "seeds:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/bin/restaurant.seed.ts --env=development",
    "seeds:prod": "NODE_ENV=production ts-node -r tsconfig-paths/register --files ./src/seeds --env=production",
    "prettier": "prettier  --config .prettierrc --write src/**/*.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "template",
    "back"
  ],
  "_moduleAliases": {
    "@constants": "dist/constants",
    "@utils": "dist/utils",
    "@log": "dist/loaders/log.loader",
    "@errors": "dist/errors",
    "@models": "dist/models",
    "@validations": "dist/validations",
    "@repositories": "dist/repositories",
    "@controllers": "dist/controllers",
    "@interfaces": "dist/interfaces",
    "@enums": "dist/enums"
  },
  "author": "Tailor-Hub",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.4.5",
    "@types/pino": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@netlify/functions": "^2.6.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^11.10.1",
    "helmet": "^7.0.0",
    "http": "^0.0.1-security",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cache": "^5.1.2",
    "node-mocks-http": "^1.12.2",
    "pino": "^8.14.2",
    "pino-pretty": "^10.2.0",
    "serverless-http": "^3.2.0",
    "tslib": "^2.6.1",
    "winston": "^3.10.0"
  }
}
