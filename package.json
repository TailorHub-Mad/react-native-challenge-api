{
  "name": "tailor_hub_technical_review",
  "version": "1.0.0",
  "description": "template back",
  "main": "src/app.ts",
  "scripts": {
    "test:coverage": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test jest --config config/jest.json --collectCoverage",
    "start": "NODE_ENV=production node -r module-alias/register dist/server.js",
    "dev": "NODE_ENV=development nodemon",
    "dev:prod": "NODE_ENV=production nodemon",
    "build": "rm -rf dist && tsc --build tsconfig.prod.json",
    "serve": "yarn run build && NODE_ENV=production node -r module-alias/register dist/server.js",
    "seed:challenge": "NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/bin/seed.challenge.ts",
    "reset:challenge": "NODE_ENV=development ts-node -r tsconfig-paths/register --files ./src/bin/reset.challenge.ts",
    "prettier": "prettier  --config .prettierrc --write src/**/*.ts",
    "lint": "eslint . --ext .ts",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "template",
    "back"
  ],
  "_moduleAliases": {
    "@constants": "dist/constants",
    "@utils": "dist/utils",
    "@log": "dist/loaders/log.loader",
    "@errors": "dist/errors",
    "@models": "dist/model",
    "@validations": "dist/validations",
    "@repositories": "dist/repository",
    "@controllers": "dist/controllers",
    "@interfaces": "dist/interfaces",
    "@enums": "dist/enums"
  },
  "author": "Tailor-Hub",
  "license": "ISC",
